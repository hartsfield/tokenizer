<script>
    $(function () {
        var $relatedExchangePairsList = $('.js-related-exchange-pairs'),
            cachedExchangePairs = {
                0: $relatedExchangePairsList.html()
            };

        $('.js-index-price-list-item').hover(function () {
            var pairId = +$(this).data('pairId');

            if (!cachedExchangePairs[pairId]) {
                $.get('/crypto/Service/getRelatedPairs',
                    {
                        p: pairId
                    },
                    function (html) {
                        cachedExchangePairs[pairId] = html;
                        $relatedExchangePairsList.html(html);
                    },
                    'html');
            } else {
                $relatedExchangePairsList.html(cachedExchangePairs[pairId]);
            }
        });
    })
</script> </div>
